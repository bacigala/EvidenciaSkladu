<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dialog.controller.FXMLItemSupplyDialogController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="129.0" prefWidth="187.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0" VBox.vgrow="NEVER">
               <children>
                  <Label fx:id="newAmountLabel" text="Množstvo" />
                  <TextField fx:id="newAmountTextField" />
               </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </VBox>
            <VBox prefHeight="150.0" prefWidth="187.0" VBox.vgrow="NEVER">
               <children>
                  <Label text="Dátum spotreby" />
                  <DatePicker fx:id="newExpirationDatePicker" prefWidth="500.0" />
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </VBox>
            <HBox alignment="CENTER" minHeight="50.0" prefHeight="51.0" prefWidth="187.0" spacing="30.0">
               <children>
                  <Button cancelButton="true" mnemonicParsing="false" onAction="#cancelButton" text="Zrušiť" />
                  <Button defaultButton="true" mnemonicParsing="false" onAction="#supplyButton" text="Vložiť" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
